            android:layout_marginLeft="32dp"
            android:layout_marginTop="15dp"
            android:layout_toEndOf="@+id/styleLabel"
            android:layout_toRightOf="@+id/styleLabel"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="0" />

        <TextView
