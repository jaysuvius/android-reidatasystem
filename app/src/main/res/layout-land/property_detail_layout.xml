            android:layout_marginLeft="32dp"
            android:layout_marginTop="15dp"
            android:layout_toEndOf="@+id/hoaFeesLabel"
            android:layout_toRightOf="@+id/hoaFeesLabel"
            android:ems="10"
            android:inputType="numberDecimal"
            android:text="0" />

        <CheckBox
            android:id="@+id/multi_checkbox"
